g:px-5 \">\n                <div class=\"flex h-12 w-12 items-center justify-center p-0 h-full w-full lg:p-0 zoom-in\">\n                  <img :alt=\"item.name\" loading=\"lazy\" decoding=\"async\" data-nimg=\"1\" class=\"drop-shadow-xl rounded rounded-xl\"\n                    :src=\"item.imageUrl\"> <!-- Langsung gunakan item.imageUrl -->\n                </div>\n                <div class=\"w-full flex flex-col gap-2 items-center text-sm md:text-base lg:text-lg\">\n                  <div class=\"title-text font-medium text-secondary\">{{ item.name }}</div>\n                  <div class=\"w-full text-left text-[10px] text-[#c1c1c1] md:text-xs lg:text-sm\">\n                    {{ item.status }}</div>\n                  <div class=\"w-full mt-4 text-normal text-sm text-left text-amber-200\">\n                    {{ item.tech }}\n                  </div>\n                  <div class=\"w-full flex justify-end\">\n                    <div class=\"flex cursor-pointer items-end gap-2 text-primary\">\n                      <a v-if=\"item.github !== 'null'\"\n                        :href=\"item.github\" target=\"_blank\" rel=\"noreferrer\"\n                        title=\"View github repository\" class=\"transition-all hover:text-accent\">\n                        <svg stroke=\"currentColor\"\n                          fill=\"none\" stroke-width=\"2\" viewBox=\"0 0 24 24\" stroke-linecap=\"round\" stroke-linejoin=\"round\"\n                          height=\"16\" width=\"16\" xmlns=\"http://www.w3.org/2000/svg\">\n                          <path\n                            d=\"M9 19c-5 1.5-5-2.5-7-3m14 6v-3.87a3.37 3.37 0 0 0-.94-2.61c3.14-.35 6.44-1.54 6.44-7A5.44 5.44 0 0 0 20 4.77 5.07 5.07 0 0 0 19.91 1S18.73.65 16 2.48a13.38 13.38 0 0 0-7 0C6.27.65 5.09 1 5.09 1A5.07 5.07 0 0 0 5 4.77a5.44 5.44 0 0 0-1.5 3.78c0 5.42 3.3 6.61 6.44 7A3.37 3.37 0 0 0 9 18.13V22\">\n                          </path>\n                        </svg>\n                      </a>\n                      <a v-if=\"item.demo !== 'null'\" :href=\"item.demo\" target=\"_blank\" rel=\"noreferrer\"\n                        title=\"View finished project\" class=\"transition-all hover:text-accent\">\n                        <svg stroke=\"currentColor\"\n                          fill=\"none\" stroke-width=\"2\" viewBox=\"0 0 24 24\" stroke-linecap=\"round\" stroke-linejoin=\"round\"\n                          height=\"18\" width=\"18\" xmlns=\"http://www.w3.org/2000/svg\">\n                          <path d=\"M18 13v6a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2V8a2 2 0 0 1 2-2h6\"></path>\n                          <polyline points=\"15 3 21 3 21 9\"></polyline>\n                          <line x1=\"10\" y1=\"14\" x2=\"21\" y2=\"3\"></line>\n                        </svg>\n                      </a>\n                    </div>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n        <div v-show=\"activeTab === 2\">\n          <div class=\"grid grid-cols-1 gap-4 pb-32 md:grid-cols-3 md:gap-3 xl:grid-cols-3 xl:gap-3 2xl:gap-5 fade-zoom-in\">\n            <div v-for=\"item in certificates\" :key=\"item.id\">\n              <div\n                class=\"item-card flex flex-col items-center gap-2 rounded bg-[#1e1e1f] hover:bg-[#282828] border border-[#383838] rounded-xl text-amber-50 md:gap-3 px-5 py-5 lg:px-5 \">\n                <div class=\"flex h-12 w-12 items-center justify-center p-0 h-full w-full lg:p-0 zoom-in\">\n                  <img :alt=\"item.name\" loading=\"lazy\" decoding=\"async\" data-nimg=\"1\" class=\"drop-shadow-xl rounded rounded-xl\"\n                    :src=\"item.imageUrl\"> <!-- Langsung gunakan item.imageUrl -->\n                </div>\n                <div class=\"w-full flex flex-col gap-2 items-center text-sm md:text-base lg:text-lg\">\n                  <div class=\"title-text font-medium text-secondary\">{{ item.name }}</div>\n                  <div class=\"w-full text-left text-[10px] text-[#c1c1c1] md:text-xs lg:text-sm\">\n                    {{ item.status }}</div>\n                  <div class=\"w-full mt-4 text-normal text-sm text-left text-amber-200\">\n                    Issuer: {{ item.issuer }}\n                  </div>\n                  <div class=\"w-full mt-2 text-normal text-sm text-left text-amber-200\">\n                    Date: {{ item.date }}\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </section>\n    </article>\n  </div>\n</template>\n<style>\n.item-card:hover {\n  transition: transform 0.3s ease;\n  transform: translateY(-8px);\n}\nsvg:hover{\n  stroke: #ffdb70;\n}\n@keyframes fadeZoomIn {\n  0% {\n    opacity: 0;\n    transform: scale(0.5);\n  }\n  100% {\n    opacity: 1;\n    transform: scale(1);\n  }\n}\n\n/* Menggunakan animasi pada elemen yang diinginkan */\n.fade-zoom-in {\n  animation: fadeZoomIn 1s ease-in-out;\n}\n</style>"],"mappings":"AACA,OAAOA,OAAM,MAAO,sBAAsB;AAC1C,OAAOC,OAAM,MAAO,sBAAsB;AAC1C,OAAOC,OAAM,MAAO,sBAAsB;AAC1C,OAAOC,OAAM,MAAO,sBAAsB;AAC1C,OAAOC,OAAM,MAAO,sBAAsB;AAC1C,OAAOC,IAAG,MAAO,kBAAkB;AACnC,OAAOC,IAAG,MAAO,kBAAkB;AACnC,OAAOC,IAAG,MAAO,iBAAiB;AAKlC,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,CAAC;MAAE;MACdC,QAAQ,EAAE,CACR;QACEC,EAAE,EAAE,CAAC;QACLC,IAAI,EAAE,aAAa;QACnBC,QAAQ,EAAER,IAAI;QACdS,MAAM,EAAE,+IAA+I;QACvJC,IAAI,EAAE,mBAAmB;QACzBC,MAAM,EAAE;MACV,CAAC,EACD;QACEL,EAAE,EAAE,CAAC;QACLC,IAAI,EAAE,eAAe;QACrBC,QAAQ,EAAEP,IAAI;QACdQ,MAAM,EAAE,oFAAoF;QAC5FC,IAAI,EAAE,mBAAmB;QACzBC,MAAM,EAAE,mDAAmD;QAC3DC,IAAI,EAAE;MACR,CAAC,EACD;QACEN,EAAE,EAAE,CAAC;QACLC,IAAI,EAAE,iBAAiB;QACvBC,QAAQ,EAAEN,IAAI;QACdO,MAAM,EAAE,qHAAqH;QAC7HC,IAAI,EAAE,mBAAmB;QACzBC,MAAM,EAAE,mDAAmD;QAC3DC,IAAI,EAAE;MACR;MACA;MAAA,CACD;;MACDC,YAAY,EAAE,CACZ;QACEP,EAAE,EAAE,CAAC;QACLC,IAAI,EAAE,uBAAuB;QAC7BC,QAAQ,EAAGb,OAAO;QAClBc,MAAM,EAAE,2DAA2D;QACnEK,MAAM,EAAE,eAAe;QACvBC,IAAI,EAAE;MACR,CAAC,EACD;QACET,EAAE,EAAE,C